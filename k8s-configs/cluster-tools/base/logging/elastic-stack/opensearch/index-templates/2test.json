{
    "index_patterns": [
      "logs-2022-02-*"
    ],
    "template": {
      "aliases": {
        "my_logs": {}
      },
      "settings": {
        "number_of_shards": 2,
        "number_of_replicas": 1,
        "index": {
          "routing": {
            "allocation": {
              "require": {
                "temp": "hot"
              }
            }
          },
          "plugins": {
            "index_state_management": {
              "rollover_alias": "2test"
            }
          }
        }
      },
      "mappings": {
        "properties": {
          "timestamp": {
            "type": "date",
            "format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
          },
          "value": {
            "type": "double"
          }
        }
      }
    }
  }