{
  "index_patterns": [
    "pd-failed-ops-*"
  ],
  "template": {
    "settings": {
      "index.number_of_shards": "3",
      "index.number_of_replicas": "1",
      "index.refresh_interval": "30s",
      "index": {
        "plugins": {
          "index_state_management": {
            "rollover_alias": "pd-failed-ops"
          }
        }
      }
    },
    "mappings": {
      "properties": {
        "geoip": {
          "dynamic": true,
          "properties": {
            "ip": {
              "type": "ip"
            },
            "latitude": {
              "type": "half_float"
            },
            "location": {
              "type": "geo_point"
            },
            "longitude": {
              "type": "half_float"
            }
          }
        },
        "timestamp": {
          "type": "text"
        },
        "cluster_name": {
          "type": "keyword"
        }
      }
    }
  },
  "composed_of": [],
  "priority": 0
}